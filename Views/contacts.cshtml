<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Contacts</title>
  </head>
  <body>
    <h1>Contact List</h1>
    <ul>
      @foreach(var contact in Model)
      {
        <li>@contact.GetLastName(), @contact.GetFirstName()</li>
      }
    </ul>
    <a href="/">Home</a>
  </body>
</html>
